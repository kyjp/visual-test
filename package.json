{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storycap": "storycap --serverCmd \"npx http-server storybook-static -p 6006\" http://localhost:6006",
    "expected": "rm -rf expected/* | mv __screenshots__/* expected/",
    "actual": "rm -rf actual/* | mv __screenshots__/* actual/",
    "new-expected": "rm -rf expected/* | mv actual/* expected/",
    "reg-cli": "npx reg-cli actual expected diff -R index.html",
    "cli-storybook": "start-storybook -p 6006 -s public",
    "cli-storycap": "storycap --serverCmd \"npm run cli-storybook\" http://localhost:6006 --serverTimeout 600000"
  },
  "dependencies": {
    "@types/node": "20.4.4",
