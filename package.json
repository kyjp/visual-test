    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "postcss": "8.4.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storycap": "^4.1.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
